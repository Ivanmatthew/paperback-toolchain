{
  "name": "@paperback/toolchain",
  "version": "0.8.0-alpha.47",
  "description": "The CLI and Build Tools for Paperback sources",
  "author": "Faizan Durrani @FaizanDurrani",
  "bin": {
    "paperback": "./bin/run"
  },
  "homepage": "https://github.com/Paperback-iOS/toolchain",
  "license": "GPL-3.0-Only",
  "main": "dist/index.js",
  "repository": "Paperback-iOS/toolchain",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "overrides": {
    "@types/cacheable-request": "^6.0.2",
    "@types/responselike": "^1.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "@oclif/core": "^1.16.1",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-plugins": "^2.1.0",
    "@paperback/runtime-polyfills": "*",
    "@paperback/types": "*",
    "@protobuf-ts/runtime-rpc": "^2.8.3",
    "@swc/core": "^1.5.24",
    "browserify": "^17.0.0",
    "chai": "^4.3.7",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "ip": "^2.0.0",
    "pug": "^3.0.2",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@protobuf-ts/plugin": "^2.8.3",
    "@protobuf-ts/protoc": "^2.8.3",
    "@types/browserify": "^12.0.37",
    "@types/chai": "^4.3.4",
    "@types/google-protobuf": "^3.15.6",
    "@types/ip": "^1.1.0",
    "@types/node": "^16.9.4",
    "@types/shelljs": "^0.8.11",
    "@types/update-notifier": "^6.0.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "globby": "^11",
    "grpc-tools": "^1.11.2",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "~5.4.5"
  },
  "oclif": {
    "bin": "paperback",
    "dirname": "paperback",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "betterbuild": "shx rm -rf dist && tsc -b && shx rm -f oflic.manifest.json",
    "build": "shx rm -rf dist && ./src/devtools/generate-ts.sh && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme && node ./bin/prepack-script",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Paperback-iOS/toolchain/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "gitHead": "f107f9f2ad6b728ab417e024ddf75765ca05c85c"
}
